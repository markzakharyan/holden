{
  "name": "vite-express-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "nodemon -w src/server -x tsx src/server/main.ts",
    "start": "NODE_ENV=production tsx src/server/main.ts",
    "start:no-redis": "NODE_ENV=production DISABLE_REDIS=true tsx src/server/main.ts",
    "build": "vite build",
    "lint": "eslint --ext .ts,.tsx src/",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:single": "vitest run -t"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/express-session": "^1.18.1",
    "@types/node": "^20.9.3",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.5.3",
    "vite": "^5.4.16",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@types/multer": "^1.4.12",
    "cheerio": "^1.0.0",
    "connect-redis": "^8.0.2",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "googleapis": "^148.0.0",
    "multer": "^1.4.5-lts.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "redis": "^4.7.0",
    "tsx": "^4.5.0",
    "typescript": "^5.3.2",
    "vite-express": "*"
  }
}
